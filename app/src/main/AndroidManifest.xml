<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rainowncorp.foodiey">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/breakfast"
        android:label="@string/app_name"
        android:roundIcon="@drawable/breakfast"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".signup"></activity>
        <activity android:name=".login" />
        <activity android:name=".checkout" />
        <activity android:name=".personal_profile" />
        <activity android:name=".selectedMenuItem" />
        <activity android:name=".SelectedRestaurant" />
        <activity android:name=".intro_screen" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>